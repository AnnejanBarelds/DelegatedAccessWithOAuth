{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "[tenant].onmicrosoft.com",
    "TenantId": "[TenantId]",
    "ClientId": "[BackendService ClientId]",
    "ClientSecret": "[BackendService Client Secret]",
    "Scopes": "access_as_user"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "SqlOptions": {
    "ConnectionString": "Server=tcp:obosqldemo.database.windows.net,1433;Initial Catalog=obodbdemo;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "StorageOptions": {
    "Url": "https://obostoragedemo.blob.core.windows.net/",
    "ContainerName": "container1"
  },
  "WeatherApiOptions": {
    "ClientId": "[WeatherApi ClientId]"
  },
  "AllowedHosts": "*"
}